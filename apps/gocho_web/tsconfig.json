{
  "extends": "tsconfig-deeple/nextjs.json",
  "compilerOptions": {
    "types": [
      "@emotion/react/types/css-prop"
    ],
    "jsxImportSource": "@emotion/react",
    "baseUrl": ".",
    "paths": {
      "@public/*": [
        "public/*"
      ],
      "@component/*": [
        "src/component/*"
      ],
      "@constant/*": [
        "src/constant/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@type/*": [
        "src/type/*"
      ],
      "@style/*": [
        "src/style/*"
      ],
      "@util/*": [
        "src/util/*"
      ],
      "@api/*": [
        "src/api/*"
      ],
      "@recoil/*": [
        "src/recoil/*"
      ],
      "@hook/*": [
        "src/hook/*"
      ]
    },
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
